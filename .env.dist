# Spotify API Configuration (Required)
REACT_APP_SPOTIFY_CLIENT_ID=
REACT_APP_SPOTIFY_REDIRECT_URL=http://127.0.0.1:3000

# OpenAI API Configuration (Required for AI Music Generation)
REACT_APP_OPENAI_API_KEY=

